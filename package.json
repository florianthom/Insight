{
    "name": "insight",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build && next export",
        "postbuild": "next-sitemap --config custom-next-sitemap.js",
        "start": "next start",
        "lint": "eslint --fix .",
        "format": "prettier --write . --config .prettierrc.js",
        "test": "jest",
        "generate-openapi": "SET JAVA_OPTS=-Dio.swagger.parser.util.RemoteUrl.trustAll=true -Dio.swagger.v3.parser.util.RemoteUrl.trustAll=true && openapi-generator-cli generate --global-property models -i https://localhost:5000/swagger/v1/swagger.json --generator-name typescript-fetch -o src/openapi_models",
        "s3-clean": "aws s3 rm s3://www.florianthom.com --recursive",
        "s3-deploy": "aws s3 sync ./out s3://www.florianthom.com"
    },
    "dependencies": {
        "@fontsource/inter": "^4.0.1",
        "@reduxjs/toolkit": "^1.8.1",
        "autoprefixer": "^10.4.4",
        "axios": "^0.21.1",
        "caniuse-lite": "^1.0.30001327",
        "jest-config": "^26.6.3",
        "next": "^12.1.6",
        "postcss": "^8.4.12",
        "react": "^18.1.0",
        "react-dom": "^18.0.0",
        "react-query": "^3.39.0",
        "react-redux": "^7.2.8",
        "sharp": "^0.30.3",
        "tailwindcss": "^3.0.23",
        "ts-node": "^9.1.1"
    },
    "devDependencies": {
        "@openapitools/openapi-generator-cli": "^2.2.6",
        "@testing-library/jest-dom": "^5.12.0",
        "@testing-library/react": "^11.2.6",
        "@testing-library/react-hooks": "^5.1.2",
        "@types/jest": "^27.4.1",
        "@types/node": "^17.0.24",
        "@types/react": "^18.0.5",
        "@types/webpack-env": "^1.16.0",
        "eslint": "^8.15.0",
        "eslint-config-next": "^12.1.6",
        "jest": "^26.6.3",
        "next-sitemap": "^2.5.20",
        "postcss-flexbugs-fixes": "^5.0.2",
        "postcss-preset-env": "^6.7.0",
        "prettier": "^2.2.1",
        "react-test-renderer": "^17.0.2",
        "ts-jest": "^27.1.4",
        "typescript": "^4.6.3"
    }
}
